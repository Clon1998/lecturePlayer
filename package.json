{
	"name": "web-player-js",
	"version": "3.0.0",
	"description": "Web player for lecture/conference/classroom recordings",
	"private": true,
	"scripts": {
		"build:dev": "webpack --config webpack.dev.js",
		"build:prod": "webpack --config webpack.prod.js",
		"watch": "webpack --config webpack.dev.js --watch",
		"start:dev": "webpack serve --host 0.0.0.0 --config webpack.dev.js",
		"type-check": "tsc --noEmit",
		"type-check:watch": "npm run type-check -- --watch",
		"generate-docs": "compodoc -p tsconfig.json"
	},
	"keywords": [
		"recording",
		"stream",
		"pdf",
		"player",
		"viewer"
	],
	"author": "Alex Andres",
	"license": "Apache-2.0",
	"devDependencies": {
		"@alienfast/i18next-loader": "^2.0.1",
		"@babel/cli": "^7.22.10",
		"@babel/core": "^7.22.10",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/plugin-proposal-decorators": "^7.22.10",
		"@babel/plugin-proposal-object-rest-spread": "^7.20.7",
		"@babel/plugin-transform-runtime": "^7.22.10",
		"@babel/preset-env": "^7.22.10",
		"@babel/preset-typescript": "^7.22.5",
		"@types/node": "^20.5.0",
		"@types/uuid": "^9.0.2",
		"babel-loader": "^9.1.3",
		"copy-webpack-plugin": "^11.0.0",
		"css-loader": "^6.8.1",
		"exports-loader": "^4.0.0",
		"lit-css-loader": "^2.0.1",
		"mini-css-extract-plugin": "^2.7.6",
		"sass": "^1.65.1",
		"sass-loader": "^13.3.2",
		"style-loader": "^3.3.3",
		"terser-webpack-plugin": "^5.3.9",
		"typescript": "^5.1.6",
		"webpack": "^5.88.2",
		"webpack-cli": "^5.1.4",
		"webpack-dev-server": "^4.15.1",
		"webpack-merge": "^5.9.0",
		"worker-loader": "^3.0.8"
	},
	"dependencies": {
		"@adobe/lit-mobx": "^2.2.0",
		"@shoelace-style/shoelace": "^2.7.0",
		"@stomp/stompjs": "^7.0.0",
		"i18next": "^23.4.4",
		"i18next-browser-languagedetector": "^7.1.0",
		"janus-gateway": "^1.2.0",
		"lit": "^2.8.0",
		"mobx": "^6.10.0",
		"pdfjs-dist": "^3.9.179",
		"uuid": "^9.0.0"
	}
}
