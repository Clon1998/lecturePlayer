{
	"name": "lecture-player",
	"version": "3.0.0",
	"description": "Web player for lecture/conference/classroom recordings",
	"private": true,
	"type": "module",
	"scripts": {
		"build:prod": "node esbuild.js",
		"build:dev": "node esbuild.js --dev",
		"watch": "node esbuild.js --dev --watch",
		"lint": "eslint --ignore-path .eslintignore --ext .ts .",
		"test": "wtr --coverage",
		"test:watch": "wtr --watch"
	},
	"keywords": [
		"recording",
		"stream",
		"pdf",
		"player",
		"viewer"
	],
	"author": "Alex Andres",
	"license": "Apache-2.0",
	"devDependencies": {
		"@open-wc/building-rollup": "^3.0.2",
		"@open-wc/testing": "^3.2.0",
		"@rollup/plugin-commonjs": "^25.0.4",
		"@rollup/plugin-replace": "^5.0.2",
		"@types/node": "^20.5.0",
		"@typescript-eslint/eslint-plugin": "^6.5.0",
		"@typescript-eslint/parser": "^6.5.0",
		"@web/dev-server-esbuild": "^0.4.1",
		"@web/dev-server-rollup": "^0.5.2",
		"@web/test-runner": "^0.17.1",
		"@web/test-runner-playwright": "^0.10.1",
		"clean-css": "^5.3.2",
		"esbuild-plugin-copy": "^2.1.1",
		"esbuild-plugin-lit-css": "^2.0.4",
		"eslint": "^8.48.0",
		"eslint-plugin-lit": "^1.9.1",
		"rollup-plugin-lit-css": "^4.0.1",
		"typescript": "^5.2.2"
	},
	"dependencies": {
		"@adobe/lit-mobx": "^2.2.0",
		"@shoelace-style/shoelace": "^2.7.0",
		"@stomp/stompjs": "^7.0.0",
		"i18next": "^23.4.4",
		"i18next-browser-languagedetector": "^7.1.0",
		"janus-gateway": "^1.2.0",
		"lit": "^2.8.0",
		"mobx": "^6.10.0",
		"pdfjs-dist": "^3.10.111",
		"typescript-event-target": "^1.0.5"
	}
